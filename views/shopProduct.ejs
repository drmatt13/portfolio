        </nav>
        <div class="advertisement-container no-select">
            <img id="banner" src="" alt="banner">
        </div>
        <div class="shop-products" id="shop-products">
            <h1><%= Object.keys(storeData)[index] %></h1>
            <div class="products">
                <% for(var i=0; i<Object.keys(storeData).length; i++) {%>
                    <div id="<%= Object.keys(storeData)[i] %>" class="item"></div>
                <% } %>
            </div>
            <div class="shop-products-container">

                <div class="testtest">

                    <div class="images-container">
                        <% for(let [i, pic] of pictures.entries()) {%>
                            <% if (i == 0) { %>
                                <div class="img no-select selected">
                                    <img src="/shop/products/<%= Object.keys(storeData)[index] %>/<%= pic %>" alt="test">
                                </div>
                            <%} else { %>
                                <div class="img no-select">
                                    <img src="/shop/products/<%= Object.keys(storeData)[index] %>/<%= pic %>" alt="test">
                                </div>
                            <% } %>
                        <% } %>
                    </div>
                    <div class="item-container">
                        <div class="item-container-image-container no-select">
                            <img src="/shop/products/<%= Object.keys(storeData)[index] %>/1.png" alt="<%= Object.keys(storeData)[index] %>">
                            <% if (soldOut) { %>
                                <div class="sold-out no-select">Sold Out</div>
                            <%} %>
                        </div>
                        <div class="item-container-footer">$<%= storeData[Object.keys(storeData)[index]].price %></div>
                    </div>

                </div>

                <div class="large-description">
                    <div><%= storeData[Object.keys(storeData)[index]].descriptionLarge %></div>
                </div>

                <div class="testtest2">
                    <div>Purple Dream</div>
                    <div>$40.00 USD</div>
                </div>

                <div class="final">


                    <div class="add-btn no-select">Add to cart</div>




                </div>

                <div class="second-large-description">
                    <div><%= storeData[Object.keys(storeData)[index]].descriptionLarge %></div>
                </div>



            </div>
        </div>
        <div class="social-container" id="social"></div>
